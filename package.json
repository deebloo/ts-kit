{
  "name": "ts-toolkit",
  "version": "1.1.1",
  "description": "A set of packages to help you be more productive",
  "scripts": {
    "DI": "---------------------------",
    "test:di": "jest --watchAll --verbose=false -c packages/di/jest.config.js",
    "test:ci:di": "jest -c packages/di/jest.config.js",
    "build:di": "ng-packagr -p packages/di/package.json",
    "release:di": "npm run build:di && node tools/update_version di && cd @ts-kit/di && npm publish --access public",
    "STATE_CONTAINER": "---------------------------",
    "test:state-container": "jest --watchAll --verbose=false -c packages/state-container/jest.config.js",
    "test:ci:state-container": "jest -c packages/state-container/jest.config.js",
    "build:state-container": "ng-packagr -p packages/state-container/package.json",
    "release:state-container": "npm run build:state-container && node tools/update_version state-container && cd @ts-kit/state-container && npm publish --access public",
    "TYPE_UTILS": "---------------------------",
    "test:type-utils": "jest --watchAll --verbose=false -c packages/di/jest.config.js",
    "test:ci:type-utils": "jest -c packages/type-utils/jest.config.js",
    "build:type-utils": "ng-packagr -p packages/type-utils/package.json",
    "release:type-utils": "npm run build:type-utils && node tools/update_version type-utils && cd @ts-kit/type-utils && npm publish --access public",
    "GENERAL": "---------------------------",
    "release": "npm run release:di && npm run release:state-container && npm run release:type-utils"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deebloo/ts-kit.git"
  },
  "author": "deebloo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deebloo/ts-kit/issues"
  },
  "devDependencies": {
    "@angular/compiler": "^7.1.0",
    "@angular/compiler-cli": "^7.1.0",
    "@angular/core": "^7.1.0",
    "@types/jest": "^23.3.9",
    "jest": "^23.6.0",
    "ng-packagr": "^4.4.0",
    "prettier": "^1.15.2",
    "ts-jest": "^23.10.4",
    "tsickle": "^0.34.0",
    "typescript": "^3.1.6",
    "rxjs": "^6.0.0"
  }
}
