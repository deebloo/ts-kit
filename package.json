{
  "name": "ts-toolkit",
  "version": "0.0.0",
  "description": "Simple state management for async data",
  "scripts": {
    "STATE_CONTAINER": "---------------------------",
    "test:state-container": "jest --watchAll --verbose=false -c packages/state-container/jest.config.js",
    "test:ci:state-container": "jest -c packages/state-container/jest.config.js",
    "build:state-container": "ng-packagr -p packages/state-container/package.json",
    "release:state-container": "npm run build:state-container && cd @ts-kit/state-container && npm release",
    "DI": "---------------------------",
    "test:di": "jest --watchAll --verbose=false -c packages/di/jest.config.js",
    "test:ci:di": "jest -c packages/di/jest.config.js",
    "build:di": "ng-packagr -p packages/di/package.json",
    "release:di": "npm run build:di && cd @ts-kit/di && npm release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deebloo/ts-kit.git"
  },
  "author": "deebloo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deebloo/ts-kit/issues"
  },
  "devDependencies": {
    "@angular/compiler": "^7.0.4",
    "@angular/compiler-cli": "^7.0.4",
    "@angular/core": "^7.1.0",
    "@types/jest": "^23.3.9",
    "jest": "^23.6.0",
    "ng-packagr": "^4.4.0",
    "prettier": "^1.15.2",
    "ts-jest": "^23.10.4",
    "tsickle": "^0.34.0",
    "typescript": "^3.1.6",
    "rxjs": "^6.0.0"
  }
}
